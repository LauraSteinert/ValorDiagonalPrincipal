/* Escreva um m√©todo que determine e retorne o valor da soma dos elementos na diagonal principal de uma matriz quadrada de inteiros. */

static int somaDiagonal(int[][] m) {
int soma = 0;
		for(int i=0;i<m.length;i++){
			for(int j=0;j<m[i].length;j++){
				if(i == j){
				soma += m[i][j];
				}
			}	
		}
return(soma);	
}
